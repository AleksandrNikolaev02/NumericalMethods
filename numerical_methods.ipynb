        "    hi = []\n",
        "    _yi_ = []\n",
        "    y_max = []\n",
        "    doubling = []\n",
        "    division = []\n",
        "    xi.append(x0)\n",
        "    yi.append(y0)\n",
        "    hi.append(h)\n",
        "    _yi_.append(y0)\n",
        "    y_max.append(0)\n",
        "    doubling.append(0)\n",
        "    division.append(0)\n",
        "    iter = 0\n",
        "    summa = x0\n",
        "    i = 1\n",
        "    while(summa < xk):\n",
        "        if(xi[i-1] + h > xk):\n",
        "          h_last = round(abs(xi[i-1] - xk), 4)\n",
        "          y = yi[i-1] + h_last * func(xi[i-1], yi[i-1])\n",
        "          h_new = h_last/2\n",
        "          x_new = xi[i-1] + h_new\n",
        "          y_new = yi[i-1] + h_new * func(xi[i-1], yi[i-1])\n",
        "          y_ = y_new + h_new * func(x_new, y_new)\n",
        "          xi.append(xi[i-1] + h_last)\n",
        "          yi.append(y)\n",
        "          hi.append(h_last)\n",
        "          _yi_.append(y_)\n",
