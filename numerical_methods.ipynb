        "def Runge_Kutta_4p_1(y0, yy0, x0, xk, e, h, Nmax, func, p=4):\n",
        "  xi = []\n",
        "  yi = []\n",
        "  ti = []\n",
        "  hi = []\n",
        "  _yi_ = []\n",
        "  y_max = []\n",
        "  _xi_ = []\n",
        "  ti.append(x0)\n",
        "  yi.append(y0)\n",
        "  _yi_.append(y0)\n",
        "  y_max.append(0)\n",
        "  xi.append(yy0)\n",
        "  _xi_.append(yy0)\n",
        "  hi.append(h)\n",
        "\n",
        "  i = 1\n",
        "  summa = x0\n",
        "  iter = 0\n",
        "  while(summa < xk):\n",
        "    ti.append(ti[i-1] + h)\n",
        "    hi.append(h)\n",
        "\n",
        "    k1 = func(ti[i-1], yi[i-1])\n",
        "    k2 = func(ti[i-1] + h/2, yi[i-1] + h/2*k1)\n",
        "    k3 = func(ti[i-1] + h/2, yi[i-1] + h/2*k2)\n",
        "    k4 = func(ti[i-1] + h, yi[i-1] + h*k3)\n",
        "    x = xi[i-1] + h/6*(k1 + 2*k2 + 2*k3 + k4)\n",
